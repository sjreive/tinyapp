    <script src="//cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <title>New URL - TinyApp Example</title>
  </head>

  <body>

    <% include partials/_header %>
    
    <div class="d-flex justify-content-center align-items-center" id="main" style= "margin-top: 30%">
        <h1 class="mr-3 pr-3 align-top border-right inline-block align-content-center"><%= errorStatusCode %></h1>
        <div class="inline-block align-middle">
          <h2 class="font-weight-normal lead" id="desc"> <%= errorMessage %></h2>
        </div>
    </div>



      <!-- Bootstrap JS -->
    <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
