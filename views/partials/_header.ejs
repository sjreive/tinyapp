<header>
    <nav class="navbar navbar-expand-md navbar-dark bg-success">
      <a class="navbar-brand" href="/urls">TinyApp</a>
      <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavAltMarkup" aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>
      <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
        <div class="navbar-nav">
          <a class="nav-item nav-link" href="/urls">My URLs</a>
          <a class="nav-item nav-link" href="/urls/new">Create New URL</a>
        </div>
      </div>
      <% if (user) {%>
        <form class="form-inline"  method="POST" action="/logout" style="margin: 0 auto">
          <label for="user" style="margin-right: 10px">Logged in as <%=user.email%></label>  
          <button type="submit" class="btn btn-primary">Logout</button></form></a> 
        
      <% } else {%>  
      <form class="form-inline"  method="GET" action="/register" style="margin: 0 auto"> 
        <button type="submit" class="btn btn-primary">Register</button></form></a>
        <form class="form-inline"  method="GET" action="/login" style="margin: 0 auto"> 
          <button type="submit" class="btn btn-primary">Login</button></form></a>
      <%}%>
    </nav>
  </header>